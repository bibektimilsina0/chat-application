{
  "name": "y",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --filter='./apps/*'",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
